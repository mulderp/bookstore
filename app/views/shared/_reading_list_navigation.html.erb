<ul>
  <h5>
    Selected Reading List:
    <%= selected_reading_list %>
  </h5>
  <li>
    Alternative reading lists:
  </li>
  <% other_reading_lists.each do |reading_list| %>
    <li>
      <%= link_to reading_list.name, reading_list_path(reading_list.id) %>
    </li>
  <% end %>
  <li>
    <%= link_to "Back to overview", root_path %> 
  </li>
</ul>
